#include "imx6ull-14x14-ddr3-arm2.dts"

&cpu0 {
	operating-points = <
		/* kHz	uV */
		996000	1275000
		792000	1225000
		528000	1175000
		396000	1025000
		198000	950000
	>;
	fsl,soc-operating-points = <
		/* KHz	uV */
		996000	1175000
		792000	1175000
		528000	1175000
		396000	1175000
		198000	1175000
	>;
	fsl,arm-soc-shared = <0>;
};

&gpc {
	fsl,ldo-bypass = <0>; /* use ldo-enable, u-boot will check it and configure */
};

&reg_arm {
	/delete-property/ vin-supply;
};

&reg_soc {
	/delete-property/ vin-supply;
};
